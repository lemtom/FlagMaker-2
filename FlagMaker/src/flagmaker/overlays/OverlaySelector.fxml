<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.VBox" id="vbox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<TabPane fx:id="tabs" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS" />
		<AnchorPane VBox.vgrow="NEVER">
			<Button fx:id="btnCancel" onAction="#Cancel" cancelButton="true" AnchorPane.rightAnchor="0"  />
		</AnchorPane>
	</children>
</fx:root>
